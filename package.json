{
  "name": "skore-challenge",
  "version": "1.0.0",
  "description": "The skore challenge project",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn --ignore-watch node_modules -r tsconfig-paths/register ./src/main/server.ts",
    "test:unit": "jest --runInBand --config=jest.config.js",
    "start": "./waiting-for.sh db:3306 --strict --timeout=300 -- npm run migration:latest && npm run dev",
    "typeorm": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -d src/adapters/typeorm-connection/ormconfig.ts",
    "migration:latest": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d src/adapters/typeorm-connection/ormconfig.ts",
    "migration:rollback": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert -d src/adapters/typeorm-connection/ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/luxon": "^2.3.2",
    "@types/sinon": "^10.0.11",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "chai": "^4.3.6",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "jest": "^29.2.2",
    "jest-ts-auto-mock": "^2.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lib": "^4.3.3",
    "luxon": "^2.4.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "npm": "^8.19.3",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.1",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.8.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-format": "^0.2.4",
    "@types/chai": "^4.3.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/uuid": "^8.3.4",
    "ttypescript": "^1.5.13"
  }
}
